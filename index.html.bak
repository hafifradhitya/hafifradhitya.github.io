        let valSubjek = inputSubjek.value
        let valPesan = inputPesan.value
        let notifPesan = document.getElementById("notifPesan")

        notifPesan.innerHTML = `Terima kasih <b>${valNama}</b>, pesan anda dari Email <b>${valEmail}</b> dengan subjek <b>${valSubjek}</b> sudah terkirim.`

        inputNama.value = ""
        inputEmail.value = ""
        inputSubjek.value = ""
        inputPesan.value = ""
    }
  </script>
  
